{
    "name": "pandamadness/devicedetector",
    "description": "Twig functions for more advanced device detection",
    "type": "bolt-extension",
    "version": "0.1.0",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.3",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Margulan Baimbet",
            "email": "thecigaroman@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\PandaMadness\\ShoppingCart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\PandaMadness\\ShoppingCart\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\PandaMadness\\DeviceDetector\\DeviceDetectorExtension"
    }
}
